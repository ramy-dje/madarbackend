NODE_ENV = "development"

MONGODB_URI = "mongodb+srv://username:password@cluster.example.mongodb.net/?retryWrites=true&w=majority"
MONGODB_DATABASE_NAME = "my-database"

# JWT
AUTH_SECRET_KEY = "example-secret-key-replace-with-strong-random-string"

# AUTH TOKENS
AUTH_ACCESS_TOKEN_EXPIRE = "15m"
AUTH_REFRESH_TOKEN_EXPIRE = "7days"

# CloudFlare R2
# Token Value
STORAGE_CLOUDFLARE_TOKEN_VALUE = "example-token-value"

# Access KEY ID & KEY SECRET
STORAGE_CLOUDFLARE_ACCESS_KEY_ID = "exampleaccesskeyid123456789"
STORAGE_CLOUDFLARE_ACCESS_KEY_SECRET = "example-access-key-secret-replace-with-actual-secret-key"

# PUBLIC URL
STORAGE_CLOUDFLARE_ACCESS_PUBLIC_URL = "https://example-public-url.r2.dev"

# S3API
STORAGE_CLOUDFLARE_S3API = "https://example.r2.cloudflarestorage.com"

# Buckets
STORAGE_CLOUDFLARE_BUCKET_NAME = "my-application-bucket"

# client_url
CLIENT_URL = "https://admin.example.com"
CLIENT_URL2 = "https://example.com"
CLIENT_URL3 = "https://www.example.com"
CLIENT_URL4 = "http://localhost:3000"

ALLOWED_SERVER_1_IP = "127.0.0.1"
ALLOWED_SERVER_2_IP = "192.168.1.1"

ALLOWED_SERVER_1_TOKEN = "example-server-1-token-replace-with-random-string"
ALLOWED_SERVER_2_TOKEN = "example-server-2-token-replace-with-random-string"
SERVER_DOMAIN = ".example.com"
SERVER_DOMAIN_DEV = ".dev-example.com"

# File Manager Client Configuration
FILE_MANAGER_API_URL=http://localhost:3000/api
FILE_MANAGER_APP_ID=example-app-id
FILE_MANAGER_APP_SECRET=example-app-secret-replace-with-strong-random-string

# nodemailer
MAIL_HOST=mail.example.com
MAIL_PORT=465
MAIL_SECURE=true
MAIL_USER=no-reply@example.com
MAIL_PASS=example-mail-password
MAIL_FROM="MyApp <no-reply@example.com>"
